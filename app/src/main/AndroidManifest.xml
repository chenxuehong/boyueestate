<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.huihe.ebai">

    <!--华为推送权限配置-->
    <permission
        android:name="com.huihe.ebai.permission.PROCESS_PUSH_MSG"
        android:protectionLevel="signatureOrSystem" />

    <uses-permission android:name="com.huihe.ebai.permission.PROCESS_PUSH_MSG" />

    <!--小米推送权限配置-->
    <permission
        android:name="com.huihe.ebai.permission.MIPUSH_RECEIVE"
        android:protectionLevel="signature" />
    <uses-permission android:name="com.huihe.ebai.permission.MIPUSH_RECEIVE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/splash"
        android:label="@string/app_name"
        android:roundIcon="@drawable/splash"
        android:supportsRtl="true"
        android:name="com.huihe.ebai.common.MainApplication"
        android:networkSecurityConfig="@xml/network"
        android:theme="@style/AppTheme">

        <activity android:name="com.huihe.ebai.ui.activity.SplashActivity"
            android:fitsSystemWindows="true"
            android:theme="@style/Theme.welcome"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="com.baidu.lbsapi.API_KEY"
            android:value="9pRyHDZbMk7RGmxbSKSVYKgOoAvqdEnh" />
        <meta-data
            android:name="com.mob.mobpush.debugLevel"
            android:value="4" />
      <!--
      多渠道打包占位符
      <meta-data
            android:name="UMENG_CHANNEL"
            android:value="${UMENG_CHANNEL_VALUE}" />-->
    </application>

</manifest>