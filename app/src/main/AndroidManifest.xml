<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.huihe.boyueestate">

    <!--华为推送权限配置-->
    <permission
        android:name="com.huihe.boyueestate.permission.PROCESS_PUSH_MSG"
        android:protectionLevel="signatureOrSystem" />
    <uses-permission android:name="com.eghuihe.teachpay.permission.PROCESS_PUSH_MSG" />

    <!--小米推送权限配置-->
    <permission
        android:name="com.huihe.boyueestate.permission.MIPUSH_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.huihe.boyueestate.permission.MIPUSH_RECEIVE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/splash_bg"
        android:label="@string/app_name"
        android:roundIcon="@drawable/splash_bg"
        android:supportsRtl="true"
        android:name=".common.MainApplication"
        android:networkSecurityConfig="@xml/network"
        android:theme="@style/AppTheme">

        <activity android:name=".ui.activity.SplashActivity"
            android:fitsSystemWindows="true"
            android:theme="@style/Theme.welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="com.baidu.lbsapi.API_KEY"
            android:value="8kutsdYWk96syU2PY8KZKWwZvImyt3Cr" />
        <meta-data
            android:name="com.mob.mobpush.debugLevel"
            android:value="4" />
    </application>

</manifest>